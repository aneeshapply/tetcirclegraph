<svg class="chart" style="height:500px; width:500px;"></svg>

<script>
  var width = 500,
    height = 500;

  var chart = d3.select(".chart")
    .attr("width", width)
    .attr("height", height);

  chart.append('circle')
    .attr('cx', 250)
    .attr('cy', 250)
    .attr('r', 220)
    .style('fill','red');
  
</script>
